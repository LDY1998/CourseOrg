{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "prefer-const": false,
    "only-arrow-functions": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*"
    ]
  }
}